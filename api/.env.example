# Application mode
NODE_ENV=development

# HTTP port
PORT=3000

# Postgres connection
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=app_db

# Logging level (fatal|error|warn|info|debug|trace)
LOG_LEVEL=info

# Comma-separated list of allowed CORS origins
CORS_ORIGINS=*

# Auth0 and jwt
AUTH0_DOMAIN=localhost
AUTH0_AUDIENCE=audience
JWT_EXPIRATION=3600
JWT_SECRET=secret

# API rate limits
RATE_LIMIT_MAX=10
RATE_LIMIT_TIME_WINDOW=60